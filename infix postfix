 int calculate(int operand 1,char operator,operand 2);
{
switch(char)
{
case "+"= return operand1 + operand 2;
case "-"= return operand1 - operand 2;
case "*"= return operand1 * operand 2;
case "/"= return operand1 / operand 2;
case "%"= return operand1 % operand 2;
}
};
